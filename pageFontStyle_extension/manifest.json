{
  "manifest_version": 2,
  "name": "PageFontStyle Extension",
  "version": "1.0",
  "description": "Changes font style on google developer page",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "page_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "PageFontStyle"
  }
}